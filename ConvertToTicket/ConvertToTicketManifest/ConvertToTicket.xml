	<HighResolutionIconUrl DefaultValue="https://defiancetech1-my.sharepoint.com/:i:/g/personal/hamri_selvi_hindujatech_com/EdjrtuAM_uVNkpp8o-ClIO4BUnbH87-d90Hze6GedmVpLQ?e=gGnFjn"/>
